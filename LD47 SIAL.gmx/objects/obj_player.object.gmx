        <arguments>
          <argument>
            <kind>1</kind>
            <string>look_dir = point_direction(x,y,mouse_x,mouse_y); //May smooth this if time is available.

var x_dir = keyboard_check(ord("D")) - keyboard_check(ord("A"));
var y_dir = keyboard_check(ord("S")) - keyboard_check(ord("W"));

if (using)
{
    x_dir = 0;
    y_dir = 0;
}

forward_distance_x = movement_speed;
forward_distance_y = movement_speed;

for (var xx = 0; xx &lt; movement_speed; xx++)
{
    if (collision_point(x + xx*x_dir,y,obj_wall,0,0) != noone)
    {
        forward_distance_x = xx-1;
        break;
    }
}

for (var yy = 0; yy &lt; movement_speed; yy++)
{
    if (collision_point(x,y + yy*y_dir,obj_wall,0,0) != noone)
    {
        forward_distance_y = yy-1;
        break;
    }
}

for (var xx = 0; xx &lt; movement_speed; xx++)
{
    if (collision_point(x + xx*x_dir,y,obj_exit_barrier,0,0) != noone)
    {
        forward_distance_x = xx-1;
        break;
    }
}

for (var yy = 0; yy &lt; movement_speed; yy++)
{
    if (collision_point(x,y + yy*y_dir,obj_exit_barrier,0,0) != noone)
    {
        forward_distance_y = yy-1;
        break;
    }
}

for (var xx = 0; xx &lt; movement_speed; xx++)
{
    if (collision_point(x + xx*x_dir,y,obj_garbage,0,0) != noone)
    {
        forward_distance_x = xx-1;
        break;
    }
}

for (var yy = 0; yy &lt; movement_speed; yy++)
{
    if (collision_point(x,y + yy*y_dir,obj_garbage,0,0) != noone)
    {
        forward_distance_y = yy-1;
        break;
    }
}
x += x_dir * forward_distance_x;
y += y_dir * forward_distance_y;

if (has_gun)
{
    sprite_index = spr_player_armed;
    if (bullets &gt; 0)
    {
        if (shoot_delay_timer-- &lt;= 0)
        {
            if (mouse_check_button_pressed(mb_left))
            {
    
                shoot_delay_timer = shoot_delay;
                var bx = x + lengthdir_x(65,look_dir);
                var by = y + lengthdir_y(65,look_dir);
                var s = instance_create(bx,by,obj_sound);
                s.snd_id = snd_gunshot;
                s.life_timer = 1*room_speed;
                var b = instance_create(bx,by,obj_bullet);
                b.spd = 16;
                b.dir = look_dir;
                bullets--;
                with (obj_gunner)
                {
                    found_player = true;        
                }
                
            }                
        }        

    }
    else
    {
        if (mouse_check_button_pressed(mb_left))
        {
            has_gun = false;
            sprite_index = spr_player;
        }
    }

}
audio_listener_position(x,y,0);

if (place_meeting(x,y,obj_dark_region)) and irandom_range(1,50) == 1
{
    with (obj_gunner)
    {
        found_player = false;        
    }
}
</string>
