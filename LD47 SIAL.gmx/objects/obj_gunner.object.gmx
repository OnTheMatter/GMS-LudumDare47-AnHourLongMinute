                shooting_player = false;
            }
        }
        else
        {
            
            target_x = random_range(0,room_width);
            target_y = random_range(0,room_height);

        
            if (collision_line(x,y,obj_player.x,obj_player.y,obj_wall,0,true) == noone)
            {
                player_in_dark = false;
                with(obj_player)
                {
                    if (place_meeting(x,y,obj_dark_region) and irandom_range(1,100) &gt; 1) other.player_in_dark = true;
                }
                if !player_in_dark
                {
                    with (obj_gunner)
                    {
                        found_player = true;         
                        path_update_timer = 1; 
                    }                
                }

    
            }    
        }     
        if (!shooting_player)
        {
            if (mp_grid_path(obj_enemy_pathfinding.pathfinding_grid,movement_path,x,y,target_x,target_y,0))
            {
                path_start(movement_path,movement_speed,path_action_stop,1);  
            }        
        }

   
